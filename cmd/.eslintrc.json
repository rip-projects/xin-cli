{
  "extends": "eslint-config-standard",
  "installedESLint": true,
  "env": {
    "node": true
  },
  "plugins": [
    "promise",
    "standard"
  ],
  "globals": {
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "strict": ["error", "global"],
    "space-before-function-paren": ["error", "never"],
    "comma-dangle": ["error", "always-multiline"]
  }
}
